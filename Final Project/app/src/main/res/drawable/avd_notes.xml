                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="400"
                    android:duration="600"
                    android:valueFrom="100"
                    android:valueTo="360"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="100"
                    android:valueFrom="0"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
